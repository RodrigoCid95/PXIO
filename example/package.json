{
	"name": "example",
	"version": "1.0.0",
	"description": "Este es un proyecto de ejemplo basado en [PX.IO](https://www.npmjs.com/package/px.io).",
	"main": "index.js",
	"scripts": {
		"init": "pxio",
		"start": "pxio start --foo bar",
		"build": "pxio build"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"body-parser": "^1.20.2",
		"compression": "^1.7.4",
		"express": "^4.18.3",
		"express-session": "^1.18.0",
		"liquidjs": "^10.10.1",
		"socket.io": "^4.7.4",
		"sqlite3": "^5.1.7",
		"uuid": "^9.0.1",
		"v4": "^0.0.1"
	},
	"pxio": {
		"mods": [
			"http",
			"sockets",
			"workers"
		],
		"boot": "manual",
		"resources": [
			"views",
			"data.db"
		],
		"loader": {
			".sql": "text"
		},
		"omitAuto": false
	},
	"devDependencies": {
		"@types/compression": "^1.7.5",
		"@types/express": "^4.17.21",
		"@types/express-session": "^1.18.0",
		"@types/express-socket.io-session": "^1.3.9",
		"@types/node": "^20.11.25",
		"@types/uuid": "^9.0.8",
		"px.io": "file:../package"
	}
}
